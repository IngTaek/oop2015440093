		//    3210123 
		//    432101234 
		//    54321012345
		System.out.print("¾È³ç");
		
		int i, j;
		
		for(j = 5; j == 0; j--)
		{
			i = j;
			
			while(i != 0)
			{
				System.out.print(i);
				
				i--;
			}
		}
		
		
		


	}

}
