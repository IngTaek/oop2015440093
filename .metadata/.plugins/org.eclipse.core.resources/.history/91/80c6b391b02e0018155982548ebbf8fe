import java.util.Scanner;

public class Six {

	public static void main(String[] args) {
		Scanner input = new Scanner(System.in);

		int number;
		int a;
		int b;
		
		System.out.print("정수를 입력하시오 : ");
		number = input.nextInt();
		
		a = number / 10;
		b = number % 10;
		
		System.out.print("출력 : " + a + b);
				
	}

}
