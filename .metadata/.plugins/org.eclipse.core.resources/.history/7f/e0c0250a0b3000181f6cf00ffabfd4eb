
public class Twentyfive_2 {

	public static void main(String[] args) {
		// (2)54321012345
		//    432101234
		//    3210123
		//    21012
		//    101
		//    0
		//    101 
		//    21012 
		//    3210123 
		//    432101234 
		//    54321012345
		System.out.print("¾È³ç");
		
		int i, j;
		
		for(j = 5; j == 0; j--)
		{
			for(i = j; i == 0; i--)
			{
				System.out.print(i);
			}
		}
		
		for(j = 1; 11-2*j > 0; j++)
		{
			for(i = 1; i <= 11-2*j; i++)
			{
				System.out.print("*");
			}
			
			System.out.print("\n");
		}
		
		for(j = 1; 2*j+1 <= 10; j++)
		{
			for(i = 1; i <= 2*j+1; i++)
			{
				System.out.print("*");
			}
			
			System.out.print("\n");
		}
		


	}

}
